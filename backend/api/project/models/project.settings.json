{
  "kind": "collectionType",
  "collectionName": "projects",
  "info": {
    "name": "project"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "name": {
      "type": "string"
    },
    "user": {
      "plugin": "users-permissions",
      "model": "user"
    },
    "version": {
      "type": "component",
      "repeatable": true,
      "component": "project.version"
    }
  }
}
